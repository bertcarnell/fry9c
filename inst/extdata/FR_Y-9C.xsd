<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <xsd:element name="FRY9C" minOccurs="1" maxOccurs="unbounded">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="schedule" minOccurs="1" maxOccurs="unbounded" type="_Schedules"/>
            </xsd:sequence>
            <xsd:attribute name="date" type="xsd:string"/>
            <xsd:attribute name="omb_number" type="xsd:string"/>
            <xsd:attribute name="title" type="xsd:string"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="_Schedules">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="component" minOccurs="1" maxOccurs="unbounded" type="_Components"/>
            </xsd:sequence>
            <xsd:attribute name="desig" type="xsd:string"/>
            <xsd:attribute name="title" type="xsd:string"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="_Components">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="component" minOccurs="0" maxOccurs="unbounded" type="_Components"/>
            </xsd:sequence>
            <xsd:attribute name="num" type="xsd:string"/>
            <xsd:attribute name="name" type="xsd:string"/>
            <xsd:attribute name="key" type="xsd:string"/>
        </xsd:complexType>
    </xsd:element>
</xsd:schema>
